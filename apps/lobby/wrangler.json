{
  "$schema": "node_modules/wrangler/config-schema.json",
  "main": ".open-next/worker.js",
  "name": "pecking-order-lobby",
  "compatibility_date": "2024-12-30",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "vars": {
    "GAME_SERVER_HOST": "https://game-server.porlock.workers.dev",
    "GAME_CLIENT_HOST": "https://pecking-order-client.pages.dev"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "pecking-order-lobby-db",
      "database_id": "placeholder-create-with-wrangler"
    }
  ]
}
