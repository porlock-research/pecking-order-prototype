{
  "name": "@pecking-order/game-cartridges",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./machines/gap-run": {
      "types": "./dist/machines/gap-run.d.ts",
      "import": "./dist/machines/gap-run.mjs"
    },
    "./machines/trivia": {
      "types": "./dist/machines/trivia.d.ts",
      "import": "./dist/machines/trivia.mjs"
    },
    "./machines/realtime-trivia": {
      "types": "./dist/machines/realtime-trivia.d.ts",
      "import": "./dist/machines/realtime-trivia.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/machines/gap-run.ts src/machines/trivia.ts src/machines/realtime-trivia.ts --format cjs,esm && tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@pecking-order/shared-types": "*"
  },
  "peerDependencies": {
    "xstate": "^5.26.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "xstate": "^5.26.0"
  }
}
